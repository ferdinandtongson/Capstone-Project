<resources>

    <style name="GymRat.FAB" parent="Base.ThemeOverlay.AppCompat.Light">
        <item name="android:layout_margin">16dp</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_gravity">bottom|right|end</item>
        <item name="android:tint">@color/fab_tint</item>
        <item name="elevation">@dimen/fab_elevation</item>
        <item name="borderWidth">@dimen/fab_borderWidth</item>
        <item name="pressedTranslationZ">@dimen/fab_pressedTranslationZ</item>
    </style>

</resources>
